{
    "name": "dyusha/laravel-html-editor",
    "description": "Laravel package that allows inline editing of HTML blocks",
    "keywords": ["laravel", "html", "content management"],
    "license": "MIT",
    "authors": [
        {
            "name": "dyusha",
            "email": "leelfosho@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0"
        },
    "autoload": {
        "psr-4": {
            "Dyusha\\HtmlEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dyusha\\HtmlEditor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    }
}
